  "scripts": {
    "execute:standard-start:testnet": "npx hardhat run scripts/standard/start-lottery.ts --network testnet",
    "execute:standard-start:mainnet": "npx hardhat run scripts/standard/start-lottery.ts --network mainnet",
    "execute:standard-close:testnet": "npx hardhat run scripts/standard/close-lottery.ts --network testnet",
