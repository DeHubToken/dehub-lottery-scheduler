    "execute:special-start:testnet": "npx hardhat run scripts/special/start-lottery.ts --network testnet",
    "execute:special-start:mainnet": "npx hardhat run scripts/special/start-lottery.ts --network mainnet",
    "execute:special-close:testnet": "npx hardhat run scripts/special/close-lottery.ts --network testnet",
    "execute:special-close:mainnet": "npx hardhat run scripts/special/close-lottery.ts --network mainnet",
    "execute:special-pick:testnet": "npx hardhat run scripts/special/pick-delotto.ts --network testnet",
    "execute:special-pick:mainnet": "npx hardhat run scripts/special/pick-delotto.ts --network mainnet"
